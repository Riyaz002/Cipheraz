<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="myETextStyle">
        <item name="android:textColorHint">@color/primaryTextColor</item>
    </style>

    <style name="Button" parent="ThemeOverlay.Material3.Button">
        <item name="android:backgroundTint">@color/secondaryColor</item>
        <item name="android:textColor">@color/secondaryTextColor</item>
    </style>

    <style name="textInputStyle1" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeWidth">2dp</item>
        <item name="errorEnabled">true</item>
        <item name="android:textColorHint">@color/primaryLightColor</item>
        <item name="hintTextColor">@color/secondaryColor</item>
        <item name="boxStrokeColor">@color/secondaryColor</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
    </style>
</resources>